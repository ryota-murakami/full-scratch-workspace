{
  "name": "full-scratch-workspace",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "styled-components": "^3.3.0"
  },
  "scripts": {
    "start": "NODE_PATH=src/ react-app-rewired start",
    "build": "NODE_PATH=src/ react-app-rewired build",
    "test": "NODE_PATH=src/ react-app-rewired test --env=jsdom",
    "test:watch": "NODE_PATH=src/ react-app-rewired test --watch --env=jsdom",
    "test:cov": "NODE_PATH=src/ yarn test --coverage",
    "lint": "NODE_PATH=src/ yarn eslint .",
    "lint:fix": "NODE_PATH=src/ yarn eslint --fix .",
    "flow": "NODE_PATH=src/ flow",
    "codecov": "codecov -t $CODECOV_TOKEN",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/**/style.js",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/element.js",
      "!src/App/type.js",
      "!src/testutil.js",
      "!src/const.js"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "codecov": "^3.0.2",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.48.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.73.0",
    "prettier": "^1.13.3",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^1.5.2",
    "react-testing-library": "^3.1.3"
  }
}
