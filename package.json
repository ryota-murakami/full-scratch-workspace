  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/cypress": "^7.0.2",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "immer": "^8.0.0",
    "react": "^17.0.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-spinners": "^0.9.0",
    "recoil": "^0.1.2",
    "styled-components": "^5.2.1",
    "swr": "^0.3.9",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.0.0-pre.59"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.18",
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.5",
    "cypress": "^6.1.0",
    "cypress-react-unit-test": "^4.17.2",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server-build": "tsc -p tsconfig.server.json",
    "server-start": "nodemon --ext ts,js,graphql --watch './backend' --ignore './src' --exec ts-node --project tsconfig.server.json ./backend/server.ts",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js.,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headed",
    "cypress:run:headless": "cypress run"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
